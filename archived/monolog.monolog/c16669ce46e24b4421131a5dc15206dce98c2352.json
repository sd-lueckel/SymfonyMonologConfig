                    "executable": false
                },
                "config/packages/prod/monolog.yaml": {
                    "contents": [
                        "monolog:",
                        "  handlers:",
                        "    main:",
                        "      type: fingers_crossed",
                        "      action_level: error",
                        "      handler: nested",
                        "      excluded_http_codes: [404, 405]",
